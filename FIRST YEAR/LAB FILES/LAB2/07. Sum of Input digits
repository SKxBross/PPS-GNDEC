#include <stdio.h>

int main() {
  int num, digit, sum = 0;

  printf("Enter a five-digit number: ");
  scanf("%d", &num);

  while (num > 0) {
    digit = num % 10; 
    sum += digit;     
    num /= 10;        
  }

  printf("The sum of the digits is: %d \n", sum);
  printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");

  return 0;
}
