#include <stdio.h>

int main() {
  int num1, num2;

  printf("Enter the first number: ");
  scanf("%d", &num1);

  printf("Enter the second number: ");
  scanf("%d", &num2);

  printf("Before swapping: num1 = %d, num2 = %d \n", num1, num2);


  printf("After swapping: num1 = %d, num2 = %d \n", num2, num1);
  printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");

  return 0;
}

#include <stdio.h>

int main() {
  int num1, num2, temp;

  printf("Enter the first number: ");
  scanf("%d", &num1);

  printf("Enter the second number: ");
  scanf("%d", &num2);

  printf("Before swapping: num1 = %d, num2 = %d \n", num1, num2);

  // Swap num1 and num2 using a third variable
  temp = num1;
  num1 = num2;
  num2 = temp;

  printf("After swapping: num1 = %d, num2 = %d \n", num1, num2);
  printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");
  return 0;
}
