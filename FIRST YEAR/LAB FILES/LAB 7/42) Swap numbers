#include <stdio.h>

void swap(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int num1, num2;

   printf("Enter value for num1: ");
    scanf("%d", &num1);

    printf("Enter value for num2: ");
    scanf("%d", &num2);

       swap(num1, num2);

     printf("\nAfter swapping :\n");
    printf("num1 = %d\n", num2);
    printf("num2 = %d\n", num1);

printf("\nWritten by Saksham Kapoor\nMEB2-2330088\n");

    return 0;
}
