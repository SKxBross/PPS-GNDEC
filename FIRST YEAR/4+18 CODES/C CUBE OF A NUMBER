#include <stdio.h>

int main() {
    int a;
    int cube;
    printf("Find Cube Of A Number \n \n");
    printf("Enter The Value You Want to Get Cube of :");
    scanf("%d", &a);
    cube = a*a*a;
    printf("Cube Of The Input Value Is : %d \n \n", cube);
    printf("Code By\n Saksham Kapoor\n 2330088\n MEB2");

 return 0;
}
