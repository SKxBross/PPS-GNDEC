#include<stdio.h>
int main()
{
    int i,j;
    int a[2][2]={1,2,3,4};
    int b[2][2]={4,3,2,1};
    int new[2][2];
    
    for(i=0;i<2;i++)
    {
      for(j=0;j<2;j++)
      
    {
    printf("sum[%d][%d]=%d\n",i+1,j+1, a[i][j] * b[i][j]);
    }


    }
    for(i=0;i<2;i++)
    {
      for(j=0;j<2;j++)
      
    {
      new[i][j]=a[i][j] * b[i][j];
      
    printf("\nNew[%d][%d]=%d\n",i+1,j+1, new[i][j]);
    }

    
    }

printf("\nCode By \n Saksham Kapoor\n MEB2 \n2330088");

return 0;
   
}
