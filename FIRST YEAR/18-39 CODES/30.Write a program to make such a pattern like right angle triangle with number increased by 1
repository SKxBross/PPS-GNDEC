#include <stdio.h>

void print_number_triangle(int n) {
    int i, j;
    for(i = 1; i <= n; i++) {
        for(j = 1; j <= i; j++) {
            printf("%d ", i);
        }
        printf("\n");
    }
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    print_number_triangle(n);
    printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");

    return 0;
}
