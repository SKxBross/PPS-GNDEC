#include <stdio.h>

void print_multiplication_table(int n) {
    int i;
    for(i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", n, i, n * i);
    }
}

int main() {
    int n;
    printf("Enter the value of n: ");
    scanf("%d", &n);

    print_multiplication_table(n);
    
    printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");

    return 0;
}
