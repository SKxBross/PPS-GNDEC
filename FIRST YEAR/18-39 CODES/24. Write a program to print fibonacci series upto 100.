#include <stdio.h>

int main() {
    int a = 0, b = 1, next;
    printf("Fibonacci Series upto 100:\n");
    printf("%d\t%d\t", a, b);
    for(int i = 0; i <= 10; i++) {
        next = a + b;
        if(next <= 100) {
            printf("%d\t", next);
        }
        a = b;
        b = next;
    }
    printf("Code By:\n Saksham Kapoor\n 2330088\n MEB2");
    return 0;
}
